{
  "version": 1,
  "framework": "nextjs",
  "imageSettings": {
    "domains": [],
    "remotePatterns": [
      {
        "protocol": "https",
        "hostname": "**.amazonaws.com"
      },
      {
        "protocol": "https",
        "hostname": "**.cloudfront.net"
      },
      {
        "protocol": "https",
        "hostname": "placehold.co"
      }
    ],
    "formats": ["image/avif", "image/webp"],
    "dangerouslyAllowSVG": false
  },
  "routes": [
    {
      "path": "/_next/static/*",
      "target": "Static",
      "cacheControl": "public,max-age=31536000,immutable"
    },
    {
      "path": "/_next/image*",
      "target": "ImageOptimization"
    },
    {
      "path": "/api/*",
      "target": "Compute"
    },
    {
      "path": "/*",
      "target": "Compute"
    }
  ],
  "computeResources": [
    {
      "name": "root",
      "runtime": "nodejs20.x",
      "entrypoint": "server.js"
    }
  ]
}
