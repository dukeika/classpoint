{
  "version": 1,
  "routes": [
    {
      "path": "/_next/static/*",
      "target": "static",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      }
    },
    {
      "path": "/_next/image",
      "target": "function",
      "src": "imageOptimizer"
    },
    {
      "path": "/*",
      "target": "function",
      "src": "default"
    }
  ],
  "functions": {
    "default": {
      "runtime": "nodejs20.x",
      "handler": "server.js",
      "memory": 512,
      "timeout": 10
    },
    "imageOptimizer": {
      "runtime": "nodejs20.x",
      "handler": "image-optimizer.js",
      "memory": 1024,
      "timeout": 10
    }
  },
  "framework": {
    "name": "nextjs",
    "version": "15.5.0"
  }
}
