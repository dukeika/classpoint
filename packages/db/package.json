    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^20.0.0",
    "prisma": "^5.21.1",
    "tsx": "^4.19.2",
    "typescript": "5.9.2"
  }
}
